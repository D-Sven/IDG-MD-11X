	<!-- HYDRAULICS -->
	<!-- Hydraulic System Control Button -->
	<animation>
		<type>pick</type>
		<object-name>SystemHyd</object-name>
		<object-name>SystemHyd.manual</object-name>
		<action>
		    <button>0</button>
		    <repeatable>false</repeatable>
		    <binding>
				<command>nasal</command>
				<script>systems.systemHydMode();</script>
		    </binding>
			<binding>
				<command>nasal</command>
				<script>setprop("/sim/sounde/oh-btn", 1);</script>
			</binding>
		</action>
	</animation>
	<animation>
		<type>select</type>
		<object-name>SystemHyd.manual</object-name>
		<condition>
			<or>
				<equals>
					<property>systems/hydraulic/system</property>
					<value>0</value>
				</equals>
				<equals>
					<property>controls/hydraulic/switches/manual-flash</property>
					<value>1</value>
				</equals>
				<equals>
					<property>controls/hydraulic/switches/manual-flash</property>
					<value>3</value>
				</equals>
				<equals>
					<property>controls/hydraulic/switches/manual-flash</property>
					<value>5</value>
				</equals>
				<equals>
					<property>controls/switches/annun-test</property>
					<value>1</value>
				</equals>
			</or>
		</condition>
	</animation>

