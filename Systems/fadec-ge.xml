<PropertyList>

    <filter>
        <name>N1 CMD 1</name>
        <type>gain</type>
        <gain>1</gain>
        <update-interval-secs type="double">0.05</update-interval-secs>
        <input>
            <expression>
                <table>
                    <property>/controls/engines/engine[0]/throttle-fdm</property>
                    <entry><ind>0.00</ind><dep> 23.7</dep></entry>
                    <entry><ind>1.00</ind><dep>115.1</dep></entry>
                </table>
            </expression>
        </input>
		<output>/engines/engine[0]/n1-cmd</output>
    </filter>
	
    <filter>
        <name>N1 CMD 2</name>
        <type>gain</type>
